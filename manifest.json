{
	"name": "Jax Messenger",
	"short_name": "Jax Messenger",
	"description": "Jax Messenger",
	"manifest_version": 2,
	"version": "0.0.1",
	"version_name": "0.0.1",
	"optional_permissions": [ "storage", "notifications", "background" ],
	"web_accessible_resources": [
		"browser-matrix.min.js"
	],
	"options_page": "index.html",
	"background": {
	  "persistent": true,
	  "scripts": ["background.js"]
	},
	"icons": {
		"16": "logo.png",
		"24": "logo.png",
		"32": "logo.png",
		"128": "logo.png"
	  },
	"browser_action": {
	  "default_icon": {
		"16": "logo.png",
		"24": "logo.png",
		"32": "logo.png",
		"128": "logo.png"
	  },
	  "default_title": "Jax",
	  "default_popup": "popup.html"
	}
}